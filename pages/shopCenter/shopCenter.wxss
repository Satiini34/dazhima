page {
  width: 100%;
  height: 100%;
  background: rgb(252, 252, 252);
}
.header_background {
  width: 100%;
  height: 165px;
  background: linear-gradient(to right, rgb(251, 192, 82),rgb(255, 136, 49));
  text-align: center;
  position: relative;
}
.header_img {
  width: 80px;
  height: 80px;
  background: #fff;
  border-radius: 50%;
  position: absolute; 
  top: -40px; 
  left: 50%;   
  transform: translateX(-50%);
  z-index: 88;
}
.head_portrait {
  width: 75px;
  height: 75px;
  border-radius: 50%;
  z-index: 99;
  background: #f1f1f1;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.check_use {
  width: 34px;
  height: 34px;
  position: absolute;
  text-align: center;
  right: 20px;
  z-index: 111;
  font-size: 11px;
}
.bottom_data {
  width: 92%;
  position: absolute;
  left: 4%;
  height: 310px;
  background: #fff;
  border-radius: 5px;
  box-shadow: 1px 3px 2px #f0f0f0;
}
.bottom_data1 {
  width: 92%;
  position: absolute;
  left: 4%;
  height: 210px;
  background: #fff;
  border-radius: 5px;
  box-shadow: 1px 3px 2px #f0f0f0;
}
.shop_name {
  position: absolute;
  width: 100%;
  height: 17px;
