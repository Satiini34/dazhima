  top: 45px; 
  font-weight: 600;
  font-size: 15px;
  font-family: Impact, Haettenschweiler, 'Arial Narrow Bold', sans-serif;
}
.shop_porfit {
  position: absolute;
  width: 100%;
  color: #ff6c00;
  font-size: 28px;
  letter-spacing: 1px;
  top: 76px;
  z-index: 99;
}
.shop_account {
  width: 100%;
  line-height: 12px;
