.topBar {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 2rpx solid #f1f1f1;
  background: #fff;
  display: flex;
  flex: 1;
  position: relative;
}
.tabBat_title {
  font-size: 30rpx;
  color: #333;
  font-family: 'lanTing';
  margin-left: 30rpx;
}
.money {
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 28rpx;
  color: #999;
  width: 416rpx;
  height: 30rpx;
  line-height: 28rpx;
  text-align: right;
}
.wechat {
  font-size: 28rpx;
  color: #999;
  margin-left: 16rpx;
  /* line-height: 90rpx; */
}
.tabBat_content {
  display: flex;
  flex: 1;
  position: absolute;
  right: 30rpx;
  /* line-height: 90rpx; */
  top: 50%;
  transform: translateY(-50%);
}
.cashout {
  width: 92%;
  height: 88rpx;
  line-height: 88rpx;
  position: relative;
  /* left: 4%; */
  margin-top: 60rpx;
  color: #fff;
  font-size: 36rpx;
  letter-spacing: 2rpx;
  border-radius: 26px;
}
.middle_info_btn {
  width: 100%;
  height: 260rpx;
  text-align: center;
  background: rgb(249, 249, 249);
}
.record {
  position: relative;
  width: 92%;
  height: 166rpx;
  left: 4%;
}
.cash_text {
  position: absolute;
  font-size: 30rpx;
  line-height: 30rpx;
  color: #333;
  top: 40rpx;
  left: 120rpx;
  font-family: 'lanTing';
  letter-spacing: 2rpx;
}
.cash_time {
  position: absolute;
  left: 120rpx;
  bottom: 34rpx;
  font-size: 26rpx; 
  color: #aaa;
  letter-spacing: 2rpx;
}
.cash_amount {
  position: absolute;
  top: 66rpx;
  right: 10rpx;
  font-size: 36rpx;
  color: #333;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}
.cash_hori {
  position: absolute;
  right: -4%;
  bottom: 0;
  width: calc(96% - 60rpx);
  height: 2rpx;
  background: rgb(235, 235, 235);
}

/* 商家 */
.shopCenter_record {
  text-align: center;
  width: 100%;
  margin-top: 40rpx;
  color: #999;
  text-decoration: underline;
  font-size: 28rpx;
  font-family: 'lanTing';
}