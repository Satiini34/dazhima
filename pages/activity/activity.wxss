}
.activity_fourth_item {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  justify-items: center;
  height: 130rpx;
}
.rangeImg {
  width:80rpx;height:80rpx;border-radius:50%;
}
.rankImgeOne {
  width:80rpx;height:80rpx;border-radius:50%;border: 2rpx solid rgb(255, 221, 58);
}
.rankNo {
  width: 10%;
  color: #FC7823;
  font-size: 40rpx;
  font-weight: bold;
  display: flex;
  justify-content: center;
}
.rankNick {
  color:#A14A10;
  font-size: 30rpx;
  font-family: 'lanTing';
}
.rankMoney {
  width: 29%;
  color: #FC7823;
  font-weight: bold;
  font-size: 30rpx;
}

.activity_canjia {
  width: 340rpx;
  height: 90rpx;
  position: absolute;
  top: 338rpx;
  left: 50%;
  transform: translateX(-50%);
}
.activity_canjiaBtn {
  font-size: 36rpx;
  color: #fff;
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  top: 14rpx;
  font-weight: bold;
}

/* 活动规则 */
.rule {
  width: 76%;
  position: fixed;
  left: 9%;
  height: 760rpx;
  top: 15%;
  background: #fff;
  padding: 3% 3%;
  color: #333;
  font-size: 26rpx;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
  border-radius: 20rpx;
  font-weight: 555;
  z-index: 999;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
.rule_title {
  font-size: 36rpx;
  font-weight: 600;
  margin:16rpx 0 16rpx 0;
  text-align: center;
}
.rule_text {
  line-height: 40rpx;
  padding: 8rpx 0;
}
.rule_dot {
  background:rgb(252, 141, 38);
  width: 12rpx;
  height: 12rpx;
  margin-right: 20rpx;
  vertical-align: 2rpx;
  border-radius: 50%;
  display: inline-block;
}