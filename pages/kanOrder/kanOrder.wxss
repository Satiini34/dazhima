page {
  width: 100%;
  height: 100%;
}
.order_shopBlock{
  width:100%;
  border-radius:5px;
  height:252rpx;
  position:relative;
  z-index:auto;
  overflow:hidden;
  background:#fff;
}
.kan_horiii {
  width: 100%;
  height: 3px;
  background: rgb(225, 225, 225);
}
.kan_horiiii {
  width: 100%;
  height: 2px;
  background: rgb(249, 249, 249);
}
.kan_coupon{
  width: 92%;
  position: relative;
  left: 3%;
  height: 100rpx;
  background: #fff;
  color: #333;
  font-size: 32rpx;
  font-weight: 600;
  line-height: 100rpx;
}
.kan_coupon_detail{
  width:92%;
  height: 80rpx;
  line-height: 80rpx; 
  position: relative;
  left: 3%;
  display: flex;
  flex: 1;
  color: #333;
  font-size: 28rpx;
}
.commentLeft {
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  color: #333;
  font-size: 26rpx;
  font-family: 'lanTing';
}
.commentRight {
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
  color: #f90;
  font-size: 26rpx;
  font-family: 'lanTing';
}
.kan_coupon_scan {
  color:rgb(255, 137, 50);
  position: absolute;
  right: 58rpx;
  font-size: 24rpx;
}
.kan_coupon_scan_used {
  color:rgb(255, 137, 50);
  position: absolute;
  right: -10rpx;
  font-size: 28rpx;
}
kan_coupon_use {
  color:rgb(255, 137, 50);
  position: absolute;
  right: -10rpx;
  font-size: 24rpx;
}
.kan_coupon_expire{
  position: absolute;
  right: -10rpx;
  font-size: 28rpx;
  color: #333;
}
.cdkey_use {
  text-decoration: line-through;
}

/* 店铺详情 */
.shopDetail {
  width: 94%;
  left: 3%;
  height: 200rpx;
  background: #fff;
  position: relative;
}
.shopDetailImg {
  width: 160rpx;
  height: 160rpx;
  position: absolute;
  left: 0;
  top: 20rpx;
  border-radius: 6rpx;
}
.shopDetailShopName {
  font-size: 24rpx;
  font-family: 'lanTing';
  color: #333;
  position: absolute;
  top: 30rpx;
  left: 190rpx;
}
.shopDetailSub {
  font-size: 20rpx;
  color: #666;
  font-family: 'lanTing';
  letter-spacing: 2rpx;
  position: absolute;
  left: 190rpx;
  top: 74rpx;
}
.shopDetailScoreStars {
  position: absolute;
  top: 104rpx;
  left: 190rpx;
}
.shopDetailLocation {
  position: absolute;
  left: 190rpx;
  bottom: 24rpx;
  font-size: 20rpx;
  font-family: 'lanTing';
  color: #999;
}
.ShopDetailShopAddress {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 112rpx;
  width: 110rpx;
  text-align: center;
  height: 30rpx;
  border-right: 2rpx solid #f3f3f3;
}
.ShopDetailShopTel {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0rpx;
  width: 110rpx;
  text-align: center;
  height: 30rpx;
}